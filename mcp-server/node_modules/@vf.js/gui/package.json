{
  "name": "@vf.js/gui",
  "version": "2.6.1",
  "description": "[https://vipkid-edu.github.io/vf-gui/](https://vipkid-edu.github.io/vf-gui/)",
  "main": "dist/gui.js",
  "types": "dist/gui.d.ts",
  "scripts": {
    "dev": "webpack --watch --config ./webpack_config/webpack.config.js",
    "release": "node buildPublish.js",
    "prod": "webpack --config ./webpack_config/publish-webpack.config",
    "build": "webpack --config ./webpack_config/webpack.config.js",
    "test": "webpack --config ./webpack_config/test-webpack.config.js --watch && npm run watch",
    "docs": "typedoc",
    "dts": "dts-generator --project  ./  --out ./dist/gui.d.ts",
    "npmPublish": "npm run release && npm publish --access public -registry=https://registry.npmjs.org",
    "vipkidNpmPublish": "npm run release && npm publish --registry=http://registry.vnpm.vipkid.com.cn"
  },
  "keywords": [
    "vf-gui",
    "typescript ui"
  ],
  "files": [
    "dist/",
    "package.json",
    "README.md"
  ],
  "author": "杨啸 <ifyx@qq.com>",
  "license": "ISC",
  "devDependencies": {
    "@microsoft/tsdoc": "^0.12.14",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "@vf.js/vf": "^6.0.2-v58",
    "dts-generator": "^3.0.0",
    "emoji-regex": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-typescript": "^0.14.0",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.3.0",
    "typedoc": "^0.15.0",
    "typescript": "^4.0.2",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vipkid-edu/vf-gui.git"
  },
  "bugs": {
    "url": "https://github.com/vipkid-edu/vf-gui/issues"
  },
  "homepage": "https://github.com/vipkid-edu/vf-gui#readme"
}
